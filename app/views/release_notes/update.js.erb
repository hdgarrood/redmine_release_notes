$('#release-notes').html('<%= escape_javascript render(:partial => 'issues/release_notes',
                                                       :locals => { :release_note => @release_note,
                                                                    :issue => @issue }) %>')
<% if @release_note.errors.any? %>
  toggle_release_notes()
<% else %>
  window.location.reload()  
<% end %>
